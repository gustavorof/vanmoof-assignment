{
    "description": "VanMoof - Product Delivery Magento 2 Module",
    "license": "Copyright",
    "name": "vanmoof/module-product-delivery",
    "type": "magento2-module",
    "authors": [
        {
            "email": "gustavor.francisco@gmail.com",
            "name": "Gustavo Rodrigues Francisco"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "magento/module-catalog": "^104.0.0"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "~1.4",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "lusitanian/oauth": "~0.8.10",
        "magento/magento-coding-standard": "*",
        "pdepend/pdepend": "~2.10.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpmd/phpmd": "^2.9.1",
        "phpstan/phpstan": "^0.12.77",
        "phpunit/phpunit": "~9.5.0",
        "sebastian/phpcpd": "^6.0.3",
        "squizlabs/php_codesniffer": "~3.6.0",
        "symfony/finder": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "VanMoof\\ProductDelivery\\": ""
        },
        "config": {
            "secure-http": false
        }
    }
}
